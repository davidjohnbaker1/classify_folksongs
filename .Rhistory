get.FANTASTIC.numeric <- function(x){
# Write all columns to object
y <- x[, .(mean.entropy, mean.productivity,mean.Simpsons.D,mean.Yules.K,mean.Sichels.S,
mean.Honores.H,p.range,p.entropy,p.std,i.abs.range,i.abs.mean,
